{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "build": "rimraf ./build && tsc"
  },
  "eslintConfig": {
    "env": {
      "es2020": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {},
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "devDependencies": {
    "@types/jsonwebtoken": "8.5.4",
    "@types/mssql": "7.1.3",
    "@types/node": "16.6.1",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "rimraf": "3.0.2",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "chalk": "4.1.2",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.1",
    "msnodesqlv8": "2",
    "mssql": "4.1.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.37"
  }
}
